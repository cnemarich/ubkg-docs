{
  "name": "sennetdocs",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "babel-polyfill": "^6.26.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-pug": "^5.0.0",
    "jquery": "^3.6.2",
    "loglevel": "^1.8.1",
    "prettier": "^2.8.1",
    "pug": "^3.0.2",
    "serve": "^14.1.2",
    "stylus": "^0.59.0"
  },
  "scripts": {
    "js": "rm -rf docs/js/lib && NODE_ENV=production babel src/js --out-dir docs/js/lib --copy-files",
    "css": "stylus --include-css --compress -w src/styles/App.styl -o docs/css/App.css",
    "build": "rm -rf docs/static && react-app-rewired build && mv build/static docs/static && cp build/index.html docs/index.html && cp build/index.html docs/404.html"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "jsxSingleQuote": true,
    "semi": false,
    "tabWidth": 4,
    "trailingComma": "none"
  },
  "dependencies": {
    "gulp-plumber": "^1.2.1"
  }
}
